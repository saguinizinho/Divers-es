//começo da herança
class People 
{
 String _name = "";
 String _email = "";
  
 void setname(String _name){
   this._name = _name;
 }
  

getname(){
    return this._name;
  }
}


class Legal extends People
{
  String _cnpj = "";
}

class Physical extends People
{
  String _cpf = "";
}

class Worker extends Physical
{
  String _enrollment = "";
}

class Student
{
  String _rm = "";
  
  
}
